{
    "name": "kaileymsnay/pcr",
    "type": "phpbb-extension",
    "description": "Allows board administrators to set post count requirements to view or post on a per-forum basis. Search results of topics/posts in post-restricted forums are removed if user doesn't have the required post count to view that forum.",
    "homepage": "https://www.snayhomelab.com/",
    "version": "1.0.0-dev",
    "time": "2021-01-01",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Kailey Snay",
            "email": "kailey.snay@snayhomelab.com",
            "homepage": "https://www.snayhomelab.com/",
            "role": "Extension Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Post Count Requirements",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
        }
    }
}
